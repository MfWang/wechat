@import "./../components/recorditem.wxss";
.record{padding-bottom:50rpx}.period{height:278rpx;border-top:1px solid rgba(0,0,0,0.12)}.period_content{margin:0 50rpx;padding:38rpx 0;display:flex;justify-content:center;align-items:center;border-bottom:1px dashed rgba(0,0,0,0.12)}.circle{width:234rpx;height:234rpx;border-radius:100%;background:#E56CAC;background-image:linear-gradient(-180deg, rgba(255,255,255,0.2) 0, rgba(255,255,255,0) 100%);box-shadow:0 7rpx 22rpx 0 rgba(0,0,0,0.05);text-align:center;display:block;margin:0 auto}.period .text{display:block;font-size:24rpx;color:#FFFFFF;padding:65rpx 0 4rpx 0}.period .day{font-size:60rpx;color:#FFFFFF;line-height:60rpx}.records_empty{padding-top:170rpx;font-size:24rpx;color:#C5C5C5;text-align:center}.records_empty .img{display:block;width:288rpx;height:124rpx;margin:0 auto 30rpx auto}